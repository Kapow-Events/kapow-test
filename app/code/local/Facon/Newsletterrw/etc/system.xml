<?xml version="1.0"?>
<config>
	<sections>
		<newsletter translate="label" module="newsletter">
			<groups>
                <subscription translate="label">
                	<fields>
                		<disablenewslettersuccesses translate="label">
                            <label>Disable the Newsletter Subscription email's success</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Disables emails send when Customer Subscribe or Unsubscribe to the Newsletter.</comment>
                        </disablenewslettersuccesses>
                	</fields>
                </subscription>
            </groups>
		</newsletter>
	</sections>
</config>